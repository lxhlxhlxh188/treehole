<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xinshuo.treehole.mapper.AdminMapper">
    <select id="getAdminByUsernameAndPassword" resultType="com.xinshuo.treehole.entity.Admin" parameterType="com.xinshuo.treehole.entity.Admin">
        select * from t_admin where name = #{name} and password = #{password}
    </select>
</mapper>